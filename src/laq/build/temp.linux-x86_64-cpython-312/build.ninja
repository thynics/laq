ninja_required_version = 1.3
cxx = c++
nvcc = /usr/local/cuda/bin/nvcc

cflags = -pthread -B /home/chenglong/anaconda3/compiler_compat -fno-strict-overflow -Wsign-compare -DNDEBUG -O2 -Wall -fPIC -O2 -isystem /home/chenglong/anaconda3/include -fPIC -O2 -isystem /home/chenglong/anaconda3/include -fPIC -I/home/chenglong/anaconda3/lib/python3.12/site-packages/torch/include -I/home/chenglong/anaconda3/lib/python3.12/site-packages/torch/include/torch/csrc/api/include -I/home/chenglong/anaconda3/lib/python3.12/site-packages/torch/include/TH -I/home/chenglong/anaconda3/lib/python3.12/site-packages/torch/include/THC -I/usr/local/cuda/include -I/home/chenglong/anaconda3/include/python3.12 -c
post_cflags = -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=laq -D_GLIBCXX_USE_CXX11_ABI=0 -std=c++17
cuda_cflags = -I/home/chenglong/anaconda3/lib/python3.12/site-packages/torch/include -I/home/chenglong/anaconda3/lib/python3.12/site-packages/torch/include/torch/csrc/api/include -I/home/chenglong/anaconda3/lib/python3.12/site-packages/torch/include/TH -I/home/chenglong/anaconda3/lib/python3.12/site-packages/torch/include/THC -I/usr/local/cuda/include -I/home/chenglong/anaconda3/include/python3.12 -c
cuda_post_cflags = -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr --compiler-options ''"'"'-fPIC'"'"'' -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=laq -D_GLIBCXX_USE_CXX11_ABI=0 -gencode=arch=compute_89,code=compute_89 -gencode=arch=compute_89,code=sm_89 -std=c++17
cuda_dlink_post_cflags = 
ldflags = 

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc --generate-dependencies-with-compile --dependency-output $out.d $cuda_cflags -c $in -o $out $cuda_post_cflags





build /home/chenglong/github.com/laq/csrc/build/temp.linux-x86_64-cpython-312/quant.o: cuda_compile /home/chenglong/github.com/laq/csrc/quant.cu






